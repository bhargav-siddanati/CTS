<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans  
               http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
	
	<bean id="qus" class="Map.Question_class">
	<constructor-arg name="id" value="121"></constructor-arg>
	<constructor-arg name="name" value="waste fellow"></constructor-arg>
	<constructor-arg name="ans" >
		<map>
			<entry key="Bhargav" value="I am the care of address of waste fellow"></entry>
			<entry key="Siddanati Bhargav" value="Me"></entry>
		</map>
	</constructor-arg>
	</bean>
	
</beans>